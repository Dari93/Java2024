public class Main {
    public static void main(String[] args) {
        // Четыре любых числа
        int num1 = 10;
        int num2 = 20;
        int num3 = 30;
        int num4 = 40;

        // Вывод чисел на экран в одну строку с тремя пробелами между ними
        System.out.println(num1 + "   " + num2 + "   " + num3 + "   " + num4);
    }
}
